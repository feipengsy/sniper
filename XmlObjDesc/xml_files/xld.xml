  <attr name="HepVector" base="CLHEP::HepGenMatrix" ns="CLHEP">
    <attr name="HepGenMatrix" ns="CLHEP" />
  </attr>
  <attr name="HepMatrix" base="CLHEP::HepGenMatrix" ns="CLHEP">
    <attr name="HepGenMatrix" ns="CLHEP" />
  </attr>
</attr>
<!--CLHEP Random end -->
<!--CLHEP Matrix start -->
<attr name="HepDiagMatrix" base="CLHEP::HepGenMatrix" ns="CLHEP">
